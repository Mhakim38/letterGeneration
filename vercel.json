{
  "functions": {
    "api/**/*.php": { 
      "runtime": "vercel-php@0.6.0"
    },
    "version": 2,
    "builds": [
        { "src": "index.php", "use": "vercel-php@0.6.0" },
      ]
  },
  "routes": [
    { "src": "/api/(.*).php", "dest": "/api/$1.php" },
    { "src": "/api/?$", "dest": "/api/index.php" }
  ]
  
}